{
    "name": "react2voby",
    "version": "1.0.6",
    "repository": "https://github.com/wongchichong/react2voby",
    "description": "",
    "main": "./dist/transform.cjs.js",
    "module": "./dist/transform.es.js",
    "browser": "./dist/transform.umd.js",
    "types": "./dist/types/transform.d.ts",
    "bin": "./cli/react2voby.mjs",
    "scripts": {
        "release": "git add . && git commit -m \"Bump version\" git push && pnpm version patch",
        "preinstall": "npx only-allow pnpm",
        "web": "vite --force --mode dev --host",
        "build": "vite build",
        "build:cli": "vite build --config vite.config.cli.ts",
        "build:lib1": "tsc --project tsconfig.build.json",
        "build:lib": "vite build --config vite.config.lib.ts",
        "watch": "tsc --watch --project tsconfig.build.json",
        "test": "node --watch ./dist/react2voby.js --config tsconfig.test.json",
        "clean": "rimraf dist cli public",
        "build:all": "pnpm clean && pnpm build && pnpm build:cli && pnpm build:lib"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/react": "^18.2.6",
        "@uiw/react-textarea-code-editor": "^2.1.1",
        "chalk": "^4.0.0",
        "fs-extra": "^11.1.1",
        "glob": "^9.3.5",
        "json5": "^2.2.3",
        "pnpm": "^8.3.1",
        "process": "^0.11.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsquery": "workspace:../tsquery",
        "typescript": "^5.0.4",
        "use-voby": "workspace:../use-voby",
        "voby": "^0.52.2",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/node": "^18.16.3",
        "@types/yargs": "^17.0.24",
        "vite": "^4.3.3",
        "vite-plugin-clean": "^1.0.0",
        "vite-plugin-dts": "^2.3.0",
        "vite-plugin-svgr": "^3.2.0"
    }
}